{
  "name": "@quest-chains/dapp",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "yarn root:prepare && next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "eslint --ignore-path .gitignore \"*/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --ignore-path .gitignore --write \"{*,**/*}.{ts,tsx,js,jsx,json,yml,yaml,md,css,scss,sol}\"",
    "root:prepare": "yarn --cwd ../../ root:prepare",
    "prepare": "yarn root:prepare",
    "generate": "graphql-codegen --config=codegen.yml"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@davatar/react": "^1.9.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@gnosis.pm/safe-apps-web3modal": "^12.0.0",
    "@quest-chains/utils": "0.1.0",
    "@superfluid-finance/sdk-core": "^0.3.2",
    "@walletconnect/web3-provider": "^1.7.8",
    "busboy": "^1.6.0",
    "chakra-ui-markdown-renderer": "^4.0.0",
    "ethers": "^5.6.4",
    "fast-memoize": "^2.5.2",
    "formik": "^2.2.9",
    "framer-motion": "^6.3.3",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-dropzone": "^12.0.5",
    "react-hot-toast": "^2.2.0",
    "react-markdown": "^8.0.3",
    "react-markdown-editor-lite": "^1.3.2",
    "react-scroll": "^1.8.7",
    "remark-gfm": "^3.0.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.51.0",
    "typescript": "^4.6.3",
    "urql": "^2.2.0",
    "web3modal": "^1.9.7"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-urql": "^3.5.7",
    "@next/eslint-plugin-next": "^12.1.5",
    "@types/busboy": "^1.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-scroll": "^1.8.3",
    "@types/remove-markdown": "^0.3.1",
    "eslint-config-next": "^12.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  }
}
